{
  "expo": {
    "name": "Ratana - Retailer Order App",
    "slug": "ratana-retailer",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/ratana.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/ratana.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ratana.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sumit9897.dude",
      "versionCode": 21,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.WAKE_LOCK"
      ],
      "buildType": "app-bundle"
    },
    "web": {
      "favicon": "./assets/images/ratana.png"
    },
    "plugins": [
      "expo-router",
      "expo-location",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/ratana.png",
          "color": "#ffffff"
        }
      ]
    ],
    "scheme": "myapp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4d06dd8d-5d6e-4333-bcc9-d5d7a21e0305"
      }
    }
  }
}